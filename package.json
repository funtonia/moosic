{
  "name": "moosic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "history": "^1.17.0",
    "morgan": "^1.6.1",
    "querystring": "^0.2.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.11",
    "request": "^2.81.0",
    "spotify-web-api-node": "^2.4.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.4.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "react-scripts": "1.0.7",
    "react-test-renderer": "^15.6.1" 
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\" \"npm run watch-css\"",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path scss src/scss/main.scss src/App.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "server": "node ./server/server.js",
    "client": "react-scripts start"
  },
  "jest": {
    "setupFiles": [
      "./tests/test-setup.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  }
}
